{
  "name": "FastCA",
  "version": "1.0",
  "author": "Junkun Lin",
  "link": "https://github.com/jkunlin/fastca",
  "input": [
    {
      "name": "model",
      "type": "file"
    },
    {
      "name": "constraint",
      "type": "file"
    }
  ],
  "output": {
    "name": "output",
    "type": "console"
  },
  "bin": "FastCA",
  "run": "FastCA [model] {[constraint]} 3600 [SEED]",
  "get_size": "awk 'END {print $(NF-1)}' [console]"
}
