{
  "name": "Tcases",
  "version": "3.7.1",
  "author": "Cornutum",
  "link": "https://github.com/Cornutum/tcases",
  "input": [
    {
      "name": "model",
      "type": "file"
    },
    {
      "name": "strength",
      "type": "number"
    }
  ],
  "output": {
    "name": "output",
    "type": "file"
  },
  "bin": "tcases/bin/tcases",
  "run": "tcases/bin/tcases -c [strength] -f [output] -r [SEED] -l stdout -T json [model]"
}