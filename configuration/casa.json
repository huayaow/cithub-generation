{
  "name": "CASA",
  "version": "1.1b",
  "author": "Brady J. Garvin",
  "link": "http://cse.unl.edu/~citportal/",
  "input": [
    {
      "name": "model",
      "type": "file"
    },
    {
      "name": "constraint",
      "type": "file"
    },
    {
      "name": "timeout",
      "type": "number"
    },
    {
      "name": "repeat",
      "type": "number"
    }
  ],
  "output": {
    "name": "output",
    "type": "file"
  },
  "bin": "casa-1.1b",
  "run": "casa-1.1b [model] {-c [constraint]} -o [output]",
  "get_size": ["sed", "-n", "1p", "[output]"]
}
